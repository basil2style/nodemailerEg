doctype html
html(lang='en')
  head
    meta(charset='utf-8')
    meta(http-equiv='X-UA-Compatible', content='IE=edge')
    meta(name='viewport', content='width=device-width, initial-scale=1')
    meta(name='description', content='Registration page for vendors.')
    meta(name='author', content='Priyank Vasa')
    link(rel='icon', href='assets/favicon.png')
    title Contact us
    // Latest compiled and minified CSS
    link(rel='stylesheet', href='https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css', integrity='sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u', crossorigin='anonymous')
    // IE10 viewport hack for Surface/desktop Windows 8 bug
    link(href='https://getbootstrap.com/assets/css/ie10-viewport-bug-workaround.css', rel='stylesheet')
    link(href='css/form.css', rel='stylesheet')
  body
    nav.navbar.navbar-inverse.navbar-fixed-top
      .container-fluid
        .navbar-header
          button.navbar-toggle(type='button', data-toggle='collapse', data-target='#myNavbar')
            span.icon-bar
            span.icon-bar
            span.icon-bar
          a.navbar-brand(href='/') InstaPay
        #myNavbar.collapse.navbar-collapse
          ul.nav.navbar-nav
            li.active
              a(href='#') Home
            li
              a(href='faq') FAQs
            li
              a(href='contactus') Contact us
          ul.nav.navbar-nav.navbar-right
            li
              a(href='/logout')
                span.glyphicon.glyphicon-log-out
                | Logout
    .container
      form(method='POST', action='contact/send')
        label.sr-only(for='fname') Name
        input#fname.form-control(name='fname', type='text', placeholder='Name', required='', autofocus='')
        label.sr-only(for='email') Email address
        input#email.form-control(name='email', type='email', placeholder='Email address', required='')
        label.sr-only(for='phone') Message
        textarea#taid.form-control(form='testformid', name='taname', cols='35', wrap='soft', placeholder='Message')
        br
        input.btn.btn-lg.btn-primary.btn-block(type='submit', value='Send')
    // /container
    script(src='https://ajax.googleapis.com/ajax/libs/jquery/3.1.1/jquery.min.js')
    // Latest compiled and minified JavaScript
    script(src='https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js', integrity='sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa', crossorigin='anonymous')
    script(src='https://getbootstrap.com/assets/js/ie-emulation-modes-warning.js')
    // HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries
    //if lt IE 9
      script(src='https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js')
      script(src='https://oss.maxcdn.com/respond/1.4.2/respond.min.js')
    // IE10 viewport hack for Surface/desktop Windows 8 bug
    script(src='https://getbootstrap.com/assets/js/ie10-viewport-bug-workaround.js')
